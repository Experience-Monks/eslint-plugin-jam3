{
  "name": "eslint-plugin-jam3",
  "version": "0.1.0",
  "description": "Jam3 eslint plugin for react",
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin",
    "eslint-plugin-jam3",
    "eslint-react"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Jam3/eslint-plugin-jam3.git"
  },
  "bugs": {
    "url": "https://github.com/Jam3/eslint-plugin-jam3/issues"
  },
  "homepage": "https://github.com/Jam3/eslint-plugin-jam3#readme",
  "author": "Jam3",
  "contributors": [
    {
      "name": "Donghyuk(Jacob) Jang",
      "url": "https://github.com/DonghyukJacobJang"
    }
  ],
  "main": "lib/index.js",
  "scripts": {
    "test": "mocha tests --recursive",
    "lint": "eslint ./",
    "pretest": "npm run lint",
    "precommit": "echo 'Pre-commit checks...' && lint-staged",
    "prepush": "echo 'Pre-push checks...' && npm run pretest"
  },
  "dependencies": {
    "requireindex": "~1.1.0"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.5",
    "eslint": "~3.9.1",
    "eslint-config-jam3": "0.1.3",
    "eslint-config-prettier": "2.9.0",
    "eslint-plugin-promise": "3.7.0",
    "eslint-plugin-import": "2.11.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-prettier": "2.6.0",
    "mocha": "^3.1.2",
    "prettier": "1.8.2"
  },
  "engines": {
    "node": ">=4"
  },
  "lint-staged": {
    "private/prj-frontend/src/**/*.{js,json,scss}": [
      "prettier --write",
      "git add"
    ]
  },
  "license": "MIT"
}
